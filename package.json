{
  "name": "base64-certificate-viewer",
  "private": true,
  "version": "1.7.0",
  "description": "You can see the details of certificates.",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "npm run dev:firefox",
    "dev:chrome": "gulp --vendor=chrome",
    "dev:firefox": "gulp --vendor=firefox",
    "dev:opera": "gulp --vendor=opera",
    "dev:edge": "gulp --vendor=edge",
    "test": "./node_modules/karma/bin/karma start karma.conf.js",
    "test:watch": "MOZ_HEADLESS=1 ./node_modules/karma/bin/karma start karma.conf.js --browsers Firefox --watch",
    "test:watchdesktop": "./node_modules/karma/bin/karma start karma.conf.js --browsers Firefox --watch",
    "lint": "standard",
    "sign": "./node_modules/web-ext/bin/web-ext sign --api-key=$AMO_API_KEY --api-secret=$AMO_API_SECRET -s dist/firefox",
    "web-ext-run": "./node_modules/web-ext/bin/web-ext run -s dist/firefox"
  },
  "standard": {
    "globals": [
      "chrome",
      "browser"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
